<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<ScrollPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainEditor.controller.MemoryController">
   <content>
      <AnchorPane minHeight="0.0" minWidth="0.0">
         <children>
            <TableView fx:id="tableViewMemory" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <columns>
                  <TableColumn fx:id="memoryAddress" prefWidth="140.0" text="Address" />
                  <TableColumn fx:id="memoryRepresentation" prefWidth="189.0" text="Representation" />
                  <TableColumn fx:id="memoryInstruction" prefWidth="268.0" text="Instruction" />
               </columns>
            </TableView>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
